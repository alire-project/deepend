project Binary_Trees_Ada2005 is

   for Object_Dir use "../obj";
   for Exec_Dir use ".";
   for Main use ("binary_trees.adb");
   for Source_Dirs use (".", "../../2005");

   package Builder is
      for Default_Switches ("ada") use ("-j4", "-g", "-o", "binary_trees_ada2005");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnatf", "-gnatwc.cfjklmp.pr.rtu.w.x", "-gnatyaAbcefhikIlnmprStux", "-gnat05", "-gnatVn", "-gnatp", "-O3", "-gnatn", "-funroll-loops", "-g");
   end Compiler;

   package Linker is
   end Linker;

   package Binder is
      for Default_Switches ("ada") use ("-E");
   end Binder;

end Binary_Trees_Ada2005;

